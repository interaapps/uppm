{
    "name": "uppm",
    "version": "2.2.1",
    "phpVersion": ">8.0",
    "repositories": [],
    "run": {
        "start": "src\/main\/bootstrap.php",
        "install": "src\/scripts\/install.php",
        "test": "src\/scripts\/testing.php"
    },
    "build": {
        "type": "phar",
        "run": "start",
        "outputName": "uppm",
        "ignored": [
            "test.txt",
            ".git",
            ".idea",
            "README.md"
        ]
    },
    "serve": {},
    "modules": {
        "interaapps\/jsonplus": "latest"
    },
    "namespaceBindings": {
        "de\\interaapps\\uppm": "src\/main\/de\/interaapps\/uppm"
    },
    "initScripts": []
}
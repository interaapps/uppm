{
    "name": "uppm",
    "version": "1.0",
    "phpVersion": ">8.0",
    "repositories": [],
    "run": {
        "start": "src\/main\/bootstrap.php",
        "install": "src\/scripts\/install.php"
    },
    "build": {
        "type": "phar",
        "run": "start",
        "outputName": "uppm",
        "ignored": [
            "test.txt",
            ".git",
            ".idea",
            "README.md"
        ]
    },
    "serve": {},
    "modules": {
        "uloleorm": "2.0.1",
        "interaapps\/jsonplus": "1.0-pre"
    },
    "namespaceBindings": {
        "de\\interaapps\\uppm": "src\/main\/de\/interaapps\/uppm"
    }
}